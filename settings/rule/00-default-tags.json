[
    {
        "metatype": "rule",
        "name": "City Tax",
        "tags": ["Stadt"],
        "filter": [{
            "key": "text_tx",
            "value": "(ABGABEN\\sLT\\.\\sBESCHEID)",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "Supermarkets",
        "tags": ["Lebensmittel"],
        "filter": [{
            "key": "text_tx",
            "value": "((?<!BY\\s)EDEKA|Wucherpfennig|REWE.*SAGT\\sDANKE|ALDI.*SAGT\\sDANKE|^Kaufland|LIDL\\sSAGT\\sDANKE)",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "Baecker",
        "tags": ["Bäcker"],
        "filter": [{
            "key": "text_tx",
            "value": "(SCHAEFERS\\sBY\\sEDEKA|MEISTERBAECKEREI|BAECKER\\sGOEING|Bäckerei|Baeckerei)",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "OnlinePayment",
        "tags": ["online"],
        "filter": [{
            "key": "text_tx",
            "value": "(PayPal\\s\\(?Europe\\)?\\sS\\.a\\.r\\.l\\.|Adyen\\sN\\.V\\.|WORLDPAY\\sLIMITED)",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "Apotheken",
        "tags": ["Apotheke"],
        "filter": [{
            "key": "text_tx",
            "value": "([Aa]potheke|APOTHEKE)",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "Kredite und Zinsen",
        "tags": ["Kredit"],
        "multi": "AND",
        "filter": [{
            "key": "text_tx",
            "value": "(Sollzins|Überziehungszins|Tilgung)",
            "compare": "regex"
        },
        {
            "key": "amount",
            "value": 0,
            "compare": "<"
        }]
    }
]