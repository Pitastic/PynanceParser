[
    {
        "metatype": "rule",
        "name": "City Tax",
        "tags": ["Stadt"],
        "filter": [{
            "key": "text_tx",
            "value": "(ABGABEN\\sLT\\.\\sBESCHEID)",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "Supermarkets",
        "tags": ["Lebensmittel"],
        "filter": [{
            "key": "text_tx",
            "value": "((?<!BY\\s)EDEKA|Wucherpfennig|REWE.*SAGT\\sDANKE|ALDI.*SAGT\\sDANKE|^Kaufland|LIDL\\sSAGT\\sDANKE|(^|\\W)(Lidl|LIDL)\\W)",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "Baecker",
        "tags": ["Bäcker"],
        "filter": [{
            "key": "text_tx",
            "value": "(SCHAEFERS\\sBY\\sEDEKA|MEISTERBAECKEREI|BAECKER\\sGOEING|Bäckerei|Baeckerei)",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "OnlinePayment",
        "tags": ["online"],
        "filter": [{
            "key": "text_tx",
            "value": "(PayPal\\s?\\(?Europe\\)?\\sS\\.a\\.r\\.l\\.|WORLDPAY\\sLIMITED|AMAZON\\s(PAYMENTS|MEDIA|.*S\\WA\\WR\\WL\\W))",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "Apotheken und Drogerien",
        "tags": ["Apotheke/Drogerie"],
        "filter": [{
            "key": "text_tx",
            "value": "([Aa]potheke|APOTHEKE|DROGERIEMARKT|ROSSMANN)",
            "compare": "regex"
        }]
    },
    {
        "metatype": "rule",
        "name": "Kredite und Zinsen",
        "tags": ["Kredit"],
        "multi": "AND",
        "filter": [{
            "key": "text_tx",
            "value": "(Sollzins|Überziehungszins|Tilgung)",
            "compare": "regex"
        },
        {
            "key": "amount",
            "value": 0,
            "compare": "<"
        }]
    },
    {
        "metatype": "rule",
        "name": "Auto Service/Tanken",
        "tags": ["Auto"],
        "filter": [{
            "key": "text_tx",
            "value": "((^|\\W)ARAL\\s|(^|\\W)ESSO\\s|(^|\\W)SHELL\\s|(^|\\W)Shell\\s|AUTOHAUS|AUTOSERVICE|TANKSTELLE|(^|\\W)TANKEN\\W)",
            "compare": "regex"
        }]
    }
]